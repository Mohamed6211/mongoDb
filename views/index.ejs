<!-- views/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Database Records</title>
</head>
<body>
  <h1>Database Records</h1>
  <ul>
    <% data.forEach(record => { %>
      <li><%= record.nom %> - <%= record.password %></li>
    <% }); %>
  </ul>
</body>
</html>
